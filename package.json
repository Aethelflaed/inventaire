    "colors": "^1.0.3",
    "config": "^1.12.0",
    "cookie-parser": "^1.3.4",
    "cookie-session": "^1.1.0",
    "cookie-signature": "^1.0.6",
    "credential": "git+https://github.com/ericelliott/credential.git",
    "express-force-ssl": "^0.2.9",
    "express-session": "^1.10.2",
    "formidable": "^1.0.17",
    "graceful-fs": "^3.0.6",
    "gravatar": "git+https://github.com/maxlath/node-gravatar.git",
    "inv-cot": "^1.0.1",
    "inv-couch": "^2.0.0",
    "inv-dbs-list": "^1.0.0",
    "inv-loggers": "^2.0.0",
    "inv-utils": "^1.3.1",
    "knox": "^0.9.2",
    "level-sublevel": "^6.4.0",
    "leveldown": "^0.10.4",
    "levelup": "^0.19.0",
