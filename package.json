{
  "name": "inventaire",
  "homepage": "https://inventaire.io",
  "version": "2.1.0",
  "description": "a libre collaborative resources mapper powered by open-knowledge (server-side)",
  "main": "index.coffee",
  "scripts": {
    "postinstall": "./scripts/postinstall",
    "install-client": "./scripts/install-client",
    "start": "coffee server.coffee",
    "superstart": "supervisor -w server,server.coffee,deploy/reload.coffee,client/app/lib/shared,config server.coffee",
    "start-prod": "./scripts/start-prod",
    "deamon-prod": "./scripts/deamon-prod",
    "test": "export NODE_ENV=tests; mocha --compilers coffee:coffee-script/register test/**/*",
    "test-watch": "export NODE_ENV=tests; mocha --compilers coffee:coffee-script/register test/**/* --watch",
    "lint": "coffeelint -f config/coffeelint.json server.coffee server/*",
    "lint-staged": "./scripts/lint-staged",
    "debug": "DEBUG=express:* coffee server.coffee",
    "update-git-hooks": "./scripts/update_git_hooks",
    "reload": "touch deploy/reload.coffee",
    "preview-email": "./scripts/preview-email",
    "patches": "./scripts/run_patches ./scripts/patches",
    "watch-i18n": "onchange server/lib/emails/i18n/src -- ./scripts/reload_i18n"
  },
  "author": "maxlath",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxlath/inventaire"
  },
  "dependencies": {
    "americano": "^0.4.2",
    "app-root-path": "^1.0.0",
    "bluebird": "^3.0.5",
    "bluereq": "^0.4.0",
    "colors": "^1.1.2",
    "config": "^1.16.0",
    "cookie-parser": "^1.4.0",
    "cookie-session": "^1.2.0",
    "cookie-signature": "^1.0.6",
    "credential": "^0.2.6",
    "email-addresses": "^2.0.1",
    "express-force-ssl": "^0.3.0",
    "express-session": "^1.11.3",
    "fastimage": "^1.0.2",
    "formidable": "^1.0.17",
    "gm": "^1.21.1",
    "graceful-fs": "^4.1.2",
    "gravatar": "^1.4.0",
    "inv-cot": "^1.1.2",
    "inv-couch": "^2.0.0",
    "inv-dbs-list": "^1.1.0",
    "inv-loggers": "^3.0.1",
    "inv-utils": "^1.4.1",
    "ip-regex": "^1.0.3",
    "isbn2": "^0.1.2",
    "knox": "^0.9.2",
    "level-sublevel": "^6.5.2",
    "leveldown": "^1.4.1",
    "levelup": "^1.2.1",
    "lodash": "^3.10.1",
    "mime": "^1.3.4",
    "moment": "^2.10.6",
    "nano": "^6.1.5",
    "node-polyglot": "^0.4.3",
    "nodemailer": "^1.10.0",
    "nodemailer-express-handlebars": "^1.0.0",
    "passport": "^0.2.2",
    "passport-browserid": "^0.1.4",
    "passport-local": "^1.0.0",
    "request": "^2.65.0",
    "serve-favicon": "^2.3.0",
    "simple-uuid": "0.0.1",
    "wikidata-sdk": "^1.4.3",
    "xml2js": "^0.4.13"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "coffee-errors": "^0.8.6",
    "coffee-script": "^1.10.0",
    "lev": "^3.2.3",
    "level-test": "^2.0.1",
    "live-server": "^0.8.2",
    "mailcomposer": "^2.1.0",
    "mocha": "^2.3.3",
    "onchange": "^2.0.0",
    "should": "^7.1.1",
    "sinon": "^1.17.1",
    "supertest": "^1.1.0",
    "supervisor": "^0.9.1",
    "trycatch": "^1.5.20"
  },
  "engines": {
    "node": "~4.2.1"
  }
