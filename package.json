    "lev": "^3.2.0",
    "level-test": "^1.6.6",
    "mocha": "^2.1.0",
    "should": "^4.0.4",
    "sinon": "^1.12.2",
    "supertest": "^0.13.0",
    "supervisor": "^0.6.0",
    "trycatch": "^1.5.11"
