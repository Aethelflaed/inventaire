{
   "_id": "_design/items",
   "language": "coffeescript",
   "views": {
       "byOwner": {
           "map": "(doc)->\n  emit(doc.owner, doc)"
       },
       "byDate": {
           "map": "(doc)->\n  emit(doc.created, [doc._id, doc.title])"
       },
       "byListing": {
           "map": "(doc)->\n\tif doc.listing?\n\t\temit [doc.owner,doc.listing], doc"
       }
   }
}